package cn.sx.decentworld.entity.server;

import java.util.List;
import java.util.Set;

/**
 * @author Sammax
 *
 */
public class MomentRefreshResult
{
    private long momentIndex;
    private long commentIndex;
    private List<Moment> newMoments;
    private List<Comment> newComments;
    private Set<String> deleteMoments;
    private Set<String> deleteComments;

    public MomentRefreshResult()
    {

    }

    public long getMomentIndex()
    {
        return momentIndex;
    }

    public void setMomentIndex(long momentIndex)
    {
        this.momentIndex = momentIndex;
    }

    public long getCommentIndex()
    {
        return commentIndex;
    }

    public void setCommentIndex(long commentIndex)
    {
        this.commentIndex = commentIndex;
    }

    public Set<String> getDeleteMoments()
    {
        return deleteMoments;
    }

    public void setDeleteMoments(Set<String> deleteMoments)
    {
        this.deleteMoments = deleteMoments;
    }

    public Set<String> getDeleteComments()
    {
        return deleteComments;
    }

    public void setDeleteComments(Set<String> deleteComments)
    {
        this.deleteComments = deleteComments;
    }

    public List<Moment> getNewMoments()
    {
        return newMoments;
    }

    public void setNewMoments(List<Moment> newMoments)
    {
        this.newMoments = newMoments;
    }

    public List<Comment> getNewComments()
    {
        return newComments;
    }

    public void setNewComments(List<Comment> newComments)
    {
        this.newComments = newComments;
    }

    @Override
    public String toString()
    {
        return "MomentRefreshResult [momentIndex=" + momentIndex + ", commentIndex=" + commentIndex + ", newMoments=" + newMoments + ", newComments=" + newComments + ", deleteMoments="
                + deleteMoments + ", deleteComments=" + deleteComments + "]";
    }

    
    
}
