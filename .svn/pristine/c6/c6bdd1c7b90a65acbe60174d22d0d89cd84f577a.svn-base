<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="721px"
    android:layout_height="594px"
    android:background="@drawable/register_dialog_bg3"
    android:layout_gravity="center_horizontal"
    android:id="@+id/root_activity_register_identify_dialog"
     >
    <LinearLayout 
        android:id="@+id/ll_register_identify_1"
        android:layout_width="360px"
        android:layout_height="60px"
        android:layout_marginTop="250px"
        android:layout_centerHorizontal="true"
        android:background="@drawable/login_et_bg"
        >
        <EditText 
            android:id="@+id/et_id_card_num"
            android:layout_width="350px"
            android:layout_height="60px"
            android:layout_marginLeft="10px"
            android:background="@null"
            android:hint="@string/input_id_card"
            android:textSize="@dimen/text_size_px_28"
            android:gravity="center"
            android:singleLine="true"
            />
    </LinearLayout>
<LinearLayout 
    	android:id="@+id/ll_register_identify_2"
        android:layout_width="360px"
        android:layout_height="60px"
        android:layout_marginTop="320px"
        android:layout_centerHorizontal="true"
        android:background="@drawable/login_et_bg"
        >
        <EditText 
            android:id="@+id/et_real_name"
            android:layout_width="350px"
            android:layout_height="60px"
            android:layout_marginLeft="10px"
            android:background="@null"
            android:hint="@string/real_name"
            android:textSize="@dimen/text_size_px_28"
            android:gravity="center"
            android:singleLine="true"
            />
    </LinearLayout>
    <Button 
        android:id="@+id/bt_to_identify"
        android:background="@drawable/login_sure_bt"
        android:layout_centerHorizontal="true"
        android:layout_width="228px"
        android:layout_height="52px"
        android:layout_marginTop="420px"
        android:text="@string/identify"
        android:textColor="@color/white"
        android:textSize="@dimen/text_size_px_28"
        android:clickable="true"
        />
    <RelativeLayout
    android:id="@+id/activity_register_identify_dialog_back"
        android:layout_width="70px"
        android:layout_height="70px"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/ll_register_identify_1"
        android:layout_marginLeft="20px"
         >
        <ImageView 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/chat_room_set_nickname_bg"
            />
    </RelativeLayout>
     <RelativeLayout
    android:id="@+id/activity_register_identify_dialog_dismiss"
        android:layout_width="70px"
        android:layout_height="70px"
        android:layout_alignParentRight="true"
        android:layout_below="@id/ll_register_identify_1"
        android:layout_marginRight="20px"
         >
        <ImageView 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/chat_room_set_nickname_bg"
            />
    </RelativeLayout>
</RelativeLayout>
