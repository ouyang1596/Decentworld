<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:paddingLeft="40dp"
    android:paddingRight="40dp" >

    <LinearLayout
        android:id="@+id/ll_talent_money_change"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="虽然我财富炫目"
            android:textColor="@color/one_black"
            android:textSize="@dimen/new_textsize_16sp" />

        <TextView
            android:id="@+id/tv_talent_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="但我才气更惊人"
            android:textColor="@color/one_black"
            android:textSize="@dimen/new_textsize_16sp" />
    </LinearLayout>

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/stroke_bg_et"
        android:hint="此处填写上传资料的名称，例如房产证"
        android:padding="10dp"
        android:textColor="@color/two_black"
        android:textSize="@dimen/new_textsize_14sp" />

    <FrameLayout
        android:id="@+id/fl_vp_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
    </FrameLayout>

    <TextView
        android:id="@+id/tv_ensure"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/bg_ensure"
        android:gravity="center"
        android:text="确认"
        android:textColor="@color/new_yellow"
        android:textSize="@dimen/new_textsize_16sp" />

</LinearLayout>