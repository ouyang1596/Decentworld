<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="@dimen/dig_width"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="@dimen/dig_margin_left_1"
            android:layout_marginRight="@dimen/dig_margin_right_1"
            android:layout_marginTop="@dimen/dig_margin_interval_1"
            android:text="确认提取"
            android:textSize="@dimen/dig_title_size" />

        <TextView
            android:id="@+id/tv_cash_draw_amount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="@dimen/dig_margin_left_1"
            android:layout_marginRight="@dimen/dig_margin_right_1"
            android:layout_marginTop="@dimen/dig_margin_interval_1"
            android:text="0.00"
            android:textColor="@color/black"
            android:textSize="@dimen/text_size_60" />

        <TextView
            android:id="@+id/tv_cash_draw_account_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="@dimen/dig_margin_left_1"
            android:layout_marginRight="@dimen/dig_margin_right_1"
            android:layout_marginTop="@dimen/dig_margin_interval_1"
            android:text="支付宝账户"
            android:textColor="#9B9B9B"
            android:textSize="@dimen/dig_title_size" />

        <TextView
            android:id="@+id/tv_cash_draw_account"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="20dp"
            android:text="138****2776"
            android:textColor="#9B9B9B"
            android:textSize="@dimen/dig_title_size" />

        <include
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="@dimen/dig_margin_interval_1"
            android:layout_marginTop="@dimen/dig_margin_interval_2"
            layout="@layout/btn_belong_dialog_submit" />
    </LinearLayout>

</LinearLayout>