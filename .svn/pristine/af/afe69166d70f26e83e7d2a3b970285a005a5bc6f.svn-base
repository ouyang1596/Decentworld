<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/iv_back"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="6dp"
                    android:paddingLeft="6dp"
                    android:paddingRight="6dp"
                    android:src="@drawable/back_doubt" />

                <TextView
                    android:id="@+id/tv_go_doubt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:text="去疑失败"
                    android:textColor="@color/two_black"
                    android:textSize="@dimen/new_textsize_16sp"
                    android:visibility="gone" />
            </LinearLayout>

            <TextView
                android:id="@+id/tv_failure_reason"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="您的资料真实，但没得异性投票者吸引"
                android:textColor="@color/two_black"
                android:textSize="@dimen/new_textsize_16sp"
                android:visibility="gone" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_doubt_finish"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/tv_try_again_buy"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:text="是否再试一次"
                android:textColor="@color/one_black"
                android:textSize="@dimen/new_textsize_16sp" />

            <TextView
                android:id="@+id/tv_complain"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:text="是否进行申诉"
                android:textColor="@color/one_black"
                android:textSize="@dimen/new_textsize_16sp"
                android:visibility="gone" />
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp" >

            <LinearLayout
                android:id="@+id/ll_ceshi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:text="不真实"
                    android:textColor="@color/one_black"
                    android:textSize="@dimen/new_textsize_16sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <SeekBar
                        android:id="@+id/sb_not_real"
                        style="@style/examine_progress"
                        android:layout_marginRight="10dp"
                        android:layout_weight="9"
                        android:thumb="@drawable/examine_fly_support" />

                    <TextView
                        android:id="@+id/tv_not_real"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="50" />
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:text="不喜欢"
                    android:textColor="@color/one_black"
                    android:textSize="@dimen/new_textsize_16sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <SeekBar
                        android:id="@+id/sb_dislike"
                        style="@style/examine_progress"
                        android:layout_marginRight="10dp"
                        android:layout_weight="9"
                        android:thumb="@drawable/examine_fly_support" />

                    <TextView
                        android:id="@+id/tv_dislike"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="50" />
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:text="支持者"
                    android:textColor="@color/one_black"
                    android:textSize="@dimen/new_textsize_16sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <SeekBar
                        android:id="@+id/sb_support"
                        style="@style/examine_progress"
                        android:layout_marginRight="10dp"
                        android:layout_weight="9"
                        android:thumb="@drawable/examine_fly_support" />

                    <TextView
                        android:id="@+id/tv_support"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="50" />
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:text="您的资料由此刻在线异性投票者决定，如果迟迟未果，容忍异性一天吧，可以再次去疑。"
                    android:textColor="@color/two_black"
                    android:textSize="@dimen/new_textsize_16sp" />
            </LinearLayout>

            <View
                android:id="@+id/view_mask"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#7fffffff"
                android:visibility="gone" />

            <ScrollView
                android:id="@+id/sclView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scrollbars="none" >

                <LinearLayout
                    android:id="@+id/ll_gv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:orientation="vertical" >

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:gravity="center"
                        android:text="支持者"
                        android:textColor="@color/one_black"
                        android:textSize="@dimen/new_textsize_16sp" />

                    <cn.sx.decentworld.widget.GridViewForScrollView
                        android:id="@+id/gv_supporters"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:listSelector="#00000000"
                        android:numColumns="3" />
                </LinearLayout>
            </ScrollView>
        </FrameLayout>
    </LinearLayout>

</RelativeLayout>