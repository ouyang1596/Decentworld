<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="559px"
    android:layout_height="736px"
    android:background="@drawable/chat_room_set_nickname_bg"
    android:id="@+id/root_activity_chat_room_set_name_dialog"
     >
    <RelativeLayout 
        android:layout_width="359px"
        android:layout_height="347px"
        android:orientation="vertical"
        android:layout_marginTop="64px"
        android:background="@color/white"
        android:layout_centerHorizontal="true"
        >
        <TextView 
            android:id="@+id/chat_room_set_nickname_tv"
            android:layout_width="270px"
            android:layout_height="85px"
            android:text="@string/set_nickname_tip"
            android:layout_alignParentTop="true"
           android:layout_centerHorizontal="true"
            android:layout_marginTop="50px"
            android:textSize="36px"
            android:gravity="center_horizontal"
            android:textStyle="bold"
            android:textColor="@color/black"
            />
        <EditText 
            android:id="@+id/chat_room_set_nickname_et"
            android:layout_alignParentTop="true"
            android:layout_marginTop="160px"
            android:layout_width="273px"
            android:layout_height="85px"
            android:layout_centerHorizontal="true"
            />
        <Button 
            android:layout_height="52px"
            android:layout_width="228px"
            android:text="@string/sure"
            android:textSize="26px"
            android:textColor="@color/white"
            android:background="@drawable/button_sure"
            android:layout_alignParentTop="true"
            android:layout_marginTop="268px"
            android:layout_centerHorizontal="true"
            />
        
    </RelativeLayout>
	<ImageView 
	    android:layout_width="153px"
	    android:layout_height="79px"
	    android:layout_alignParentTop="true"
	    android:layout_centerHorizontal="true"
	    android:src="@drawable/fate_mask"
	    />
	<ImageView 
	    android:layout_width="46px"
	    android:layout_height="45px"
	    android:layout_centerHorizontal="true"
	    android:layout_marginTop="610px"
	    android:src="@drawable/cancel_solid_icon"
	    
	    />
</RelativeLayout>
