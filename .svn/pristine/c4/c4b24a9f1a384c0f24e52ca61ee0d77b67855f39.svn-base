<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_privacy_setting_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/divide_line_transparency"
    android:orientation="vertical" >

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/divide_line_transparency"
        android:padding="10dp"
        android:text="@string/phone_number"
        android:textColor="@color/two_black"
        android:visibility="gone"
        android:textSize="@dimen/new_textsize_14sp" />

    <LinearLayout
        android:id="@+id/privacy_setting_modification_phone_number"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/white"
        android:clickable="true"
        android:visibility="gone"
        android:gravity="center"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/tv_phone_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="139 *** ** 262"
            android:textColor="@color/two_black"
            android:textSize="@dimen/new_textsize_14sp" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/divide_line_transparency"
        android:padding="10dp"
        android:text="@string/password_"
        android:textColor="@color/two_black"
        android:textSize="@dimen/new_textsize_14sp" />

    <LinearLayout
        android:id="@+id/privacy_setting_modification_password"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/white"
        android:clickable="true"
        android:gravity="center"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/privacy_setting_password"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="修改密码"
            android:textColor="@color/two_black"
            android:textSize="@dimen/new_textsize_14sp" />
    </LinearLayout>

</LinearLayout>