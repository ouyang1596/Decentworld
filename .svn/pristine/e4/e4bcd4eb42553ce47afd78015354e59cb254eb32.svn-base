<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="测试文件上传" />

    <Button
        android:id="@+id/btn_test_create_file"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="创建文件文件上传" />

    <Button
        android:id="@+id/btn_test_upload_file"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="测试文件上传" />

    <Button
        android:id="@+id/btn_test_create_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="写日志1" />

    <Button
        android:id="@+id/btn_test_create_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="写日志2" />

</LinearLayout>