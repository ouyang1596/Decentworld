    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="330px"
        android:layout_height="60px"
		android:layout_marginLeft="160px"
        android:layout_centerVertical="true"
        android:background="@drawable/login_et_bg" >

        <EditText
            android:id="@+id/et_nickname"
            android:layout_width="320px"
            android:layout_height="60px"
            android:layout_marginLeft="10px"
            android:background="@null"
            android:gravity="center"
            android:hint="@string/set_nickname"
            android:singleLine="true"
            android:textSize="@dimen/text_size_px_28" />
    </LinearLayout>

    <Button
        android:id="@+id/bt_sure_nickname"
        android:layout_width="200px"
        android:layout_height="52px"
		android:layout_marginLeft="230px"
        android:layout_marginTop="360px"
        android:background="@drawable/login_sure_bt"
        android:clickable="true"
        android:text="@string/make_sure"
        android:textColor="@color/white"
        android:textSize="@dimen/text_size_px_28" />
<RelativeLayout
    android:id="@+id/activity_register_nick_name_dialog_back"
        android:layout_width="70px"
        android:layout_height="70px"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="300px"
        android:layout_marginLeft="20px"
         >
        <ImageView 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/chat_room_set_nickname_bg"
            />
    </RelativeLayout>
 

</RelativeLayout>