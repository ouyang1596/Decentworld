package cn.sx.decentworld.entity;

/**
 * @ClassName: MatchSuccessEntity.java
 * @Description: 匹配成功后将该对象传递给ConversationEngine生成会话列表及系统消息
 * @author: cj
 * @date: 2016年4月7日 下午8:26:46
 */
public class MatchSuccessEntity
{
    //对方的ID
    public String dwID;
    //对方的名字
    public String name;
    //对方的用户类型
    public int userType;
    //对方的身价
    public float worth;
    
    /**
     * @param dwID
     * @param name
     * @param userType
     * @param worth
     */
    public MatchSuccessEntity(String dwID, String name, int userType, float worth)
    {
        super();
        this.dwID = dwID;
        this.name = name;
        this.userType = userType;
        this.worth = worth;
    }


    /**
     * @return the dwID
     */
    public String getDwID()
    {
        return dwID;
    }


    /**
     * @param dwID the dwID to set
     */
    public void setDwID(String dwID)
    {
        this.dwID = dwID;
    }


    /**
     * @return the name
     */
    public String getName()
    {
        return name;
    }


    /**
     * @param name the name to set
     */
    public void setName(String name)
    {
        this.name = name;
    }


    /**
     * @return the userType
     */
    public int getUserType()
    {
        return userType;
    }


    /**
     * @param userType the userType to set
     */
    public void setUserType(int userType)
    {
        this.userType = userType;
    }


    /**
     * @return the worth
     */
    public float getWorth()
    {
        return worth;
    }


    /**
     * @param worth the worth to set
     */
    public void setWorth(float worth)
    {
        this.worth = worth;
    }
    

}
