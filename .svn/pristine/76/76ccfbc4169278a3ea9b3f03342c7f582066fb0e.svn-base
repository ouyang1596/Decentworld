<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app1="http://schemas.android.com/apk/res/cn.sx.decentworld"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="62dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="62dp"
        android:background="@drawable/bg_white_translucent_radius" />

    <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="52dp"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="18dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/bg_white_translucent_radius" />

    <TextView
        android:id="@+id/tv_refresh"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="150dp"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="35dp"
        android:layout_marginTop="2dp"
        android:gravity="center"
        android:text="点击刷新"
        android:textColor="@color/two_black"
        android:textSize="@dimen/new_textsize_16sp" />

    <cn.sx.decentworld.flingswipe.SwipeFlingAdapterView
        android:id="@+id/fragment_stranger_near_lv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app1:rotation_degrees="2" />

</FrameLayout>